ngx_addon_name=ngx_http_speed_detection_module
 
if test -n "$ngx_module_link"; then
   # The New Way
   ngx_module_type=HTTP_FILTER
   ngx_module_name=ngx_http_speed_detection_module
   ngx_module_srcs="$ngx_addon_dir/ngx_http_speed_detection_module.c"
 
   . auto/module
else
   # The Old Way
   HTTP_FILTER_MODULES="$HTTP_FILTER_MODULES ngx_http_speed_detection_module"
   NGX_ADDON_SRCS="$NGX_ADDON_SRCS $ngx_addon_dir/ngx_http_speed_detection_module.c"
fi
